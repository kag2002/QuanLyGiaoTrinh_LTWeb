@model BTL.Models.ChiTietHstra

@{
    Layout = "~/Views/Shared/Giaotrinh.cshtml";

}

<div class="col-md-12">
    <h1>Thêm chi tiết hồ sơ trả</h1>
</div>

    <div class="col-md-4">
        <form asp-action="ThemChiTietHoSoTra">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
             <input asp-for="MaHstra" class="form-control" />
          
            <div class="form-group">
                <label asp-for="MaGt" class="control-label"></label>
            <select asp-for="MaGt" class="form-control" asp-items="ViewBag.MaGt"></select>
         
            </div>
            <div class="form-group">
                <label asp-for="MaViPham" class="control-label"></label>
                <select asp-for="MaViPham" class ="form-control" asp-items="ViewBag.MaViPham"></select>
            </div>
            <div class="form-group">
                <label asp-for="MaPhat" class="control-label"></label>
                <select asp-for="MaPhat" class ="form-control" asp-items="ViewBag.MaPhat"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>

